                </fo:list-item>
                <fo:list-item>
                  <fo:list-item-label>
                    <fo:block></fo:block>
                  </fo:list-item-label>
                  <fo:list-item-body>
                    <fo:block>FX97 4TU</fo:block>
                  </fo:list-item-body>
                </fo:list-item>
              </fo:list-block>
            </fo:block></fo:block-container>
            <fo:block-container width="20%" left="80%" top="0in" position="absolute"><fo:block>
              <fo:list-block>
                <fo:list-item>
                  <fo:list-item-label>
                    <fo:block></fo:block>
                  </fo:list-item-label>
                  <fo:list-item-body>
                    <fo:block>NIC&amp;EO</fo:block>
                  </fo:list-item-body>
                </fo:list-item>
                <fo:list-item>
