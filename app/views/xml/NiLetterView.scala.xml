              </fo:block>
            </fo:block></fo:block-container>
          </fo:block-container>
          <fo:block-container height="1in">
            <fo:block font-size="18px">@messages("label.information_is_available_in_large_print_audio_tape_and_braille_formats")</fo:block>
            <fo:block font-size="18px">@messages("label.text_relay_service_prefix_number_18001")</fo:block>
          </fo:block-container>
        </fo:block>
      </fo:block-container>

    </fo:flow>

  </fo:page-sequence>
</fo:root>


                          

