                  @listItem(messages("label.your_right_to_work_in_the_uk"))
                </fo:list-block>
              </fo:block>

              <fo:block font-weight="bold" margin-bottom="0.06in" margin-top="0.15in">@messages("label.child.trust.fund")</fo:block>
              <fo:block>@messages("label.child.trust.fund.details")</fo:block>
              <fo:block font-weight="bold" margin-bottom="0.06in" margin-top="0.15in">@messages("label.welsh_language")</fo:block>
