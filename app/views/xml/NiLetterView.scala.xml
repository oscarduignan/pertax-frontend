                @listItem(messages("label.email.for.welsh.language"))
                @listItem(messages("label.phone.number.for.welsh.language"))
                </fo:list-block>
              </fo:block>
            </fo:block>
            </fo:block-container>
            <fo:block-container width="36%" left="64%" top="0in" position="absolute" background-color="LightGreen" padding="0.1in"><fo:block>
              <fo:block font-weight="bold" margin-bottom="0.06in" border-bottom="solid 2px green">@messages("label.now_you_have_got_your") @messages("label.national_insurance_number")</fo:block>
              <fo:block margin-top="0.015in" margin-bottom="0.06in" border-bottom="solid 2px green">@messages("label.you_can_download_and_use_the_hmrc_app_or_go_online_to")
