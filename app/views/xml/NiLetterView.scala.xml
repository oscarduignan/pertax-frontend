@this(listItem: ListItem)

@(implicit messages: Messages)

<fo:root xmlns:fo="http://www.w3.org/1999/XSL/Format"
         xmlns:fox="http://xmlgraphics.apache.org/fop/extensions"
         xml:lang="en"
         font-family="LiberationSans">
  <fo:layout-master-set>
    <fo:simple-page-master master-name="main" page-height="297mm" page-width="210mm" margin-top="1cm" margin-bottom="1cm" margin-left="2cm" margin-right="2cm">
      <fo:region-body region-name="xsl-region-body" margin-top="3cm" margin-bottom="1cm"/>
      <fo:region-before region-name="xsl-region-head"/>
      <fo:region-after region-name="xsl-region-foot"/>
    </fo:simple-page-master>
  </fo:layout-master-set>
  <fo:declarations>
    <x:xmpmeta xmlns:x="adobe:ns:meta/">
      <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
        <rdf:Description rdf:about=""
                         xmlns:dc="http://purl.org/dc/elements/1.1/">
          <dc:title>National Insurance letter</dc:title>
          <dc:creator>Hmrc</dc:creator>
          <dc:description>Your National Insurance number</dc:description>
        </rdf:Description>
        <rdf:Description rdf:about=""
                         xmlns:xmp="http://ns.adobe.com/xap/1.0/">
          <xmp:CreatorTool>Hmrc</xmp:CreatorTool>
        </rdf:Description>
      </rdf:RDF>
    </x:xmpmeta>
  </fo:declarations>
  <fo:page-sequence master-reference="main">
    <fo:title>National insurance letter</fo:title>

    <fo:static-content flow-name="xsl-region-head">

      <fo:block-container width="100%">
        <fo:block-container left="0in" top="0in" position="absolute" width="30%">
          <fo:block>
            <fo:external-graphic fox:alt-text="Hmrc logo" src="url('data:image/jpeg;base64,')"/>
          </fo:block>
          <fo:block>HM Revenue</fo:block>
          <fo:block>&amp; Customs</fo:block>
        </fo:block-container>

        <fo:block-container left="2in" top="0.7in" position="absolute" width="70%">
          <fo:block text-align="right" font-size="18pt" font-weight="bold"><fo:inline color="green">Your National Insurance number</fo:inline></fo:block>
        </fo:block-container>
      </fo:block-container>
    </fo:static-content>

    <fo:static-content flow-name="xsl-region-foot">
      <fo:block-container width="50%" left="0in" top="0in" position="absolute">
        <fo:block text-align="left" font-size="12pt">Page <fo:page-number/></fo:block>
      </fo:block-container>
      <fo:block-container width="50%" left="50%" top="0in" position="absolute">
        <fo:block text-align="right">HMRC 04/22</fo:block>
      </fo:block-container>
    </fo:static-content>

