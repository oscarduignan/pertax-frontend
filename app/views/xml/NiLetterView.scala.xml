                    <fo:block></fo:block>
                  </fo:list-item-label>
                  <fo:list-item-body>
                    <fo:block>BX9 1AN</fo:block>
                  </fo:list-item-body>
                </fo:list-item>
                <fo:list-item>
                  <fo:list-item-label>
                    <fo:block></fo:block>
                  </fo:list-item-label>
                  <fo:list-item-body>
                    <fo:block>0300 200 3500</fo:block>
                  </fo:list-item-body>
                </fo:list-item>
                <fo:list-item>
                  <fo:list-item-label>
                    <fo:block></fo:block>
                  </fo:list-item-label>
                  <fo:list-item-body>
                    <fo:block>www.gov.uk/hmrc</fo:block>
                  </fo:list-item-body>
                </fo:list-item>
              </fo:list-block>
            </fo:block></fo:block-container>
          </fo:block-container>
