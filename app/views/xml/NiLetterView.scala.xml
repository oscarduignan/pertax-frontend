            <fo:block text-align="center" font-size="18px" font-weight="bold">AA000003A</fo:block>
          </fo:block-container>
          <fo:block-container height="0.5in">
            <fo:block text-align="center" font-weight="bold">@messages("label.keep_this_number_in_a_safe_place_do_not_destroy_this_letter")</fo:block>
          </fo:block-container>
          <fo:block-container height="4.8in">
            <fo:block-container width="62%" left="0in" top="0in" position="absolute" margin-right="0.1in"><fo:block>
              <fo:block font-weight="bold" margin-bottom="0.06in">@messages("label.about_your_national_insurance_number")</fo:block>
              <fo:block>@messages("label.your_national_insurance_number_is_unique_to_you_and_will_never_change_")</fo:block>
              <fo:block>@messages("label.you_will_need_it_if_you")
                <fo:list-block provisional-distance-between-starts="0.3cm" provisional-label-separation="0.15cm">
                  @listItem(messages("label.start_work_including_part_time_and_weekend_jobs"))
                  @listItem(messages("label.apply_for_a_driving_licence"))
                  @listItem(messages("label.apply_for_a_student_loan"))
                  @listItem(messages("label.claim_state_benefits"))
                  @listItem(messages("label.register_to_vote"))
